{
  "name": "axio-plus",
  "version": "0.3.0",
  "description": "axio-plus",
  "main": "lib/index.js",
  "scripts": {
    "build:lib": "node_modules/.bin/tsc",
    "build:prod:min": "node_modules/.bin/webpack --config config/webpack.prod.js",
    "build": "npm run clean && npm run build:lib && npm run build:prod:min",
    "clean": "rm -rf lib dist",
    "test": "node_modules/.bin/jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superTerrorist/axio-plus.git"
  },
  "keywords": [
    "http",
    "axio",
    "rxjs"
  ],
  "author": "david wang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/superTerrorist/axio-plus/issues"
  },
  "homepage": "https://github.com/superTerrorist/axio-plus#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "rxjs": "^6.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/jest": "^24.0.11",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.0.0",
    "typescript": "^3.0.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7",
    "webpack-merge": "^4.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
